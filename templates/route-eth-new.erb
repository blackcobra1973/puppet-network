<% if !@ipv4_routes.empty? -%>
<% [@ipv4_routes].flatten.each do |ipv4_route| -%>
<%= ipv4_route %> dev <%= @interface %>
<% end -%>
<% end -%>
<% if !@ipv6_routes.empty? -%>
<% [@ipv6_routes].flatten.each do |ipv6_route| -%>
<%= ipv6_route %> dev <%= @interface %>
<% end -%>
<% end -%>
